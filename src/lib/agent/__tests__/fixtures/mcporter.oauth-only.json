{
  "mcpServers": {
    "oauth-server": {
      "description": "Server requiring OAuth authentication",
      "baseUrl": "https://api.example.com/mcp",
      "auth": "oauth",
      "tokenCacheDir": "~/.mcporter/test-oauth"
    },
    "another-oauth": {
      "description": "Another OAuth server",
      "baseUrl": "https://api2.example.com/mcp",
      "requiresOAuth": true,
      "oauth": {
        "authUrl": "https://auth.example.com/authorize",
        "tokenUrl": "https://auth.example.com/token"
      }
    }
  }
}
