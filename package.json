{
  "name": "ispo-code",
  "version": "1.0.0",
  "description": "Multi-agent control panel for AI coding agents (Claude, Codex, OpenCode, Cerebras)",
  "type": "module",
  "bin": {
    "ispo": "./dist/cli/bin.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "dev": "concurrently -n streams,vite -c magenta,cyan \"tsx scripts/streams-worker.ts\" \"SKIP_STREAMS_INIT=true vite dev\"",
    "dev:vite": "vite dev",
    "dev:streams": "tsx scripts/streams-worker.ts",
    "dev:cli": "tsx bin/ispo.ts",
    "build": "npm run build:app && npm run build:cli",
    "build:app": "vite build",
    "build:cli": "tsup",
    "start": "vite preview",
    "start:cli": "node dist/cli/bin.js",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.8",
    "@ai-sdk/openai": "^3.0.12",
    "@cerebras/cerebras_cloud_sdk": "^1.64.1",
    "@durable-streams/client": "^0.1.5",
    "@durable-streams/server": "^0.1.6",
    "@durable-streams/state": "^0.1.5",
    "@opencode-ai/sdk": "^1.1.19",
    "@pierre/diffs": "^1.0.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-query": "^5.62.16",
    "@tanstack/react-router": "^1.149.3",
    "@tanstack/react-router-ssr-query": "^1.149.3",
    "@tanstack/react-start": "^1.149.4",
    "@trpc/client": "^11.4.3",
    "@trpc/react-query": "^11.4.3",
    "@trpc/server": "^11.4.3",
    "ai": "^6.0.35",
    "chalk": "^5.6.2",
    "class-variance-authority": "^0.7.1",
    "cli-table3": "^0.6.5",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cookie": "^1.1.1",
    "diff": "^8.0.3",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.26.2",
    "iron-session": "^8.0.4",
    "isomorphic-dompurify": "^2.35.0",
    "lucide-react": "^0.561.0",
    "octokit": "^5.0.5",
    "open": "^11.0.0",
    "ora": "^9.0.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.3",
    "react-diff-view": "^3.3.2",
    "react-dom": "^19.2.3",
    "shiki": "^3.21.0",
    "streamdown": "^2.0.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.0.6",
    "ts-pattern": "^5.9.0",
    "tw-animate-css": "^1.2.9",
    "use-debounce": "^10.1.0",
    "vinxi": "^0.5.10",
    "zod": "^3.25.76",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.5",
    "@types/node": "^22.10.5",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  }
}
